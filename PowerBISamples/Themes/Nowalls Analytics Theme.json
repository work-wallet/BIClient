{
    "name": "NoWalls Analytics",
    "background": "#333333",
    "foreground": "#FFFFFF",
    "tableAccent": "#FFFFFF",
    "dataColors": [
        "#99ca3b",
        "#666666",
        "#999999",
        "#B3B3B3",
        "#CCCCCC",
        "#8AD4EB",
        "#FE9666",
        "#A66999",
        "#3599B8",
        "#DFBFBF",
        "#4AC5BB",
        "#5F6B6D",
        "#FB8281",
        "#F4D25A",
        "#7F898A",
        "#A4DDEE",
        "#FDAB89",
        "#B687AC",
        "#28738A",
        "#A78F8F",
        "#168980",
        "#293537",
        "#BB4A4A",
        "#B59525",
        "#475052",
        "#6A9FB0",
        "#BD7150",
        "#7B4F71",
        "#1B4D5C",
        "#706060",
        "#0F5C55",
        "#1C2325"
    ],
    "visualStyles": {
        "*": {
            "*": {
                "labels": [
                    {
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "fontSize": 8,
                        "fontFamily": "Segoe UI"
                    }
                ],
                "title": [
                    {
                        "show": true,
                        "fontColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "fontSize": 13,
                        "fontFamily": "Segoe UI"
                    }
                ],
                "legend": [
                    {
                        "show": true,
                        "position": "Left",
                        "showTitle": false,
                        "labelColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "fontSize": 8,
                        "fontFamily": "Segoe UI light"
                    }
                ],
                "valueAxis": [
                    {
                        "show": true,
                        "axisScale": "linear",
                        "labelColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "color": {
                            "solid": {
                                "color": "#454545"
                            }
                        },
                        "fontSize": 8.25,
                        "fontFamily": "Segoe UI"
                    }
                ],
                "categoryAxis": [
                    {
                        "show": true,
                        "labelColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "fontSize": 8.25,
                        "fontFamily": "Segoe UI"
                    }
                ],
                "*": [
                    {
                        "fontSize": 10,
                        "fontColor": [
                            {
                                "solid": {
                                    "color": "#FFFFFF"
                                }
                            },
                            {
                                "solid": {
                                    "color": "#FFFFFF"
                                }
                            }
                        ],
                        "labelColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "fontFamily": "Segoe UI light",
                        "color": {
                            "solid": {}
                        },
                        "*": [
                            {
                                "responsive": true,
                                "wordWrap": true,
                                "backgroundColor": {
                                    "solid": {
                                        "color": "#FFFFFF"
                                    }
                                },
                                "fontSize": 11,
                                "fontColor": {
                                    "solid": {
                                        "color": "#FFFFFF"
                                    }
                                },
                                "labelColor": {
                                    "solid": {
                                        "color": "#FFFFFF"
                                    }
                                },
                                "background": [
                                    {
                                        "show": true,
                                        "transparency": 0
                                    }
                                ]
                            }
                        ],
                        "labels": [
                            {
                                "show": true,
                                "textSize": 12,
                                "labelOrientation": "vertical"
                            }
                        ]
                    }
                ],
                "general": [
                    {
                        "responsive": true
                    }
                ],
                "background": [
                    {
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#454545"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "visualTooltip": [
                    {
                        "fontSize": 7.5
                    }
                ],
                "outspacePane": [
                    {
                        "transparency": 100,
                        "foregroundColor": {
                            "solid": {
                                "color": "#DDDDDD"
                            }
                        },
                        "titleSize": 23,
                        "headerSize": 8
                    }
                ],
                "filterCard": [
                    {
                        "$id": "Available",
                        "backgroundColor": {
                            "solid": {
                                "color": "#666666"
                            }
                        },
                        "foregroundColor": {
                            "solid": {
                                "color": "#E3E3E3"
                            }
                        },
                        "transparency": 100
                    },
                    {
                        "$id": "Applied",
                        "backgroundColor": {
                            "solid": {
                                "color": "#454545"
                            }
                        },
                        "transparency": 100,
                        "foregroundColor": {
                            "solid": {
                                "color": "#99CA3B"
                            }
                        }
                    }
                ]
            }
        },
        "card": {
            "*": {
                "labels": [
                    {
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "fontSize": 12,
                        "fontFamily": "Segoe UI"
                    }
                ]
            }
        },
        "slicer": {
            "*": {
                "*": [
                    {
                        "fontSize": 10,
                        "fontColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        }
                    }
                ],
                "items": [
                    {
                        "background": {
                            "solid": {
                                "color": "#666666"
                            }
                        },
                        "textSize": 8
                    }
                ],
                "general": [
                    {
                        "responsive": true
                    }
                ],
                "categoryAxis": [
                    {
                        "fontSize": 7.5
                    }
                ],
                "valueAxis": [
                    {
                        "fontSize": 7.5
                    }
                ],
                "visualTooltip": [
                    {
                        "fontSize": 7.5
                    }
                ]
            }
        },
        "tableEx": {
            "*": {
                "grid": [
                    {
                        "gridVertical": true,
                        "gridVerticalColor": {
                            "solid": {
                                "color": "#666666"
                            }
                        },
                        "gridVerticalWeight": 1,
                        "gridHorizontal": true,
                        "gridHorizontalColor": {
                            "solid": {
                                "color": "#666666"
                            }
                        },
                        "gridHorizontalWeight": 1,
                        "outlineColor": {
                            "solid": {
                                "color": "#99ca3b"
                            }
                        }
                    }
                ],
                "values": [
                    {
                        "fontSize": 10,
                        "valuesFontColour": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "backColorPrimary": {
                            "solid": {
                                "color": "#454545"
                            }
                        },
                        "backColorSecondary": {
                            "solid": {
                                "color": "#454545"
                            }
                        }
                    }
                ],
                "total": [
                    {
                        "backgroundColor": {
                            "solid": {
                                "color": "#333333"
                            }
                        }
                    }
                ],
                "columnHeaders": [
                    {
                        "fontColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "backColor": {
                            "solid": {
                                "color": "#333333"
                            }
                        },
                        "alignment": "Center",
                        "autoSizeColumnWidth": false,
                        "fontFamily": "Segoe UI",
                        "fontSize": 8
                    }
                ],
                "background": [
                    {
                        "show": false,
                        "color": {
                            "solid": {
                                "color": "#454545"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        },
        "pivotTable": {
            "*": {
                "grid": [
                    {
                        "gridVertical": true,
                        "gridVerticalColor": {
                            "solid": {
                                "color": "#666666"
                            }
                        },
                        "gridVerticalWeight": 1,
                        "gridHorizontal": true,
                        "gridHorizontalColor": {
                            "solid": {
                                "color": "#666666"
                            }
                        },
                        "gridHorizontalWeight": 1,
                        "outlineColor": {
                            "solid": {
                                "color": "#99ca3b"
                            }
                        }
                    }
                ],
                "values": [
                    {
                        "fontSize": 10,
                        "valuesFontColour": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "backColorPrimary": {
                            "solid": {
                                "color": "#454545"
                            }
                        },
                        "backColorSecondary": {
                            "solid": {
                                "color": "#454545"
                            }
                        }
                    }
                ],
                "columnHeaders": [
                    {
                        "fontColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "backColor": {
                            "solid": {
                                "color": "#333333"
                            }
                        },
                        "alignment": "Center",
                        "autoSizeColumnWidth": false,
                        "fontFamily": "Segoe UI",
                        "fontSize": 10
                    }
                ],
                "total": [
                    {
                        "fontColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "backColor": {
                            "solid": {
                                "color": "#666666"
                            }
                        },
                        "fontFamily": "Segoe UI",
                        "fontSize": 8
                    }
                ],
                "rowHeaders": [
                    {
                        "fontColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "backColor": {
                            "solid": {
                                "color": "#333333"
                            }
                        },
                        "alignment": "Left",
                        "autoSizeColumnWidth": false,
                        "fontFamily": "Segoe UI",
                        "fontSize": 8
                    }
                ],
                "background": [
                    {
                        "show": false,
                        "color": {
                            "solid": {
                                "color": "#454545"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        },
        "shapes": {
            "*": [
                {
                    "fill": false
                }
            ]
        },
        "page": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#333333"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "wallPaper": [
                    {
                        "color": {
                            "solid": {
                                "color": "#333333"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "outspace": [
                    {
                        "color": {
                            "solid": {
                                "color": "#333333"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "outspacePane": [
                    {
                        "backgroundColor": {
                            "solid": {
                                "color": "#333333"
                            }
                        },
                        "foregroundColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "transparency": 0,
                        "titleSize": 26,
                        "headerSize": 9,
                        "fontFamily": "Segoe UI",
                        "border": true,
                        "borderColor": {
                            "solid": {
                                "color": "#454545"
                            }
                        }
                    }
                ]
            }
        }
    },
    "textClasses": {
        "label": {
            "color": "#FFFFFF",
            "fontSize": 12,
            "fontFace": "'Segoe UI', wf_segoe-ui_normal, helvetica, arial, sans-serif"
        },
        "title": {
            "color": "#FFFFFF",
            "fontSize": 12
        },
        "callout": {
            "color": "#FFFFFF",
            "fontSize": 12
